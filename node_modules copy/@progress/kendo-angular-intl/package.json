{
  "name": "@progress/kendo-angular-intl",
  "version": "14.0.0",
  "description": "Kendo UI Internationalization for Angular components",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Progress",
  "homepage": "https://www.telerik.com/kendo-angular-ui/components/",
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "keywords": [
    "Angular",
    "Internationalization",
    "Kendo UI",
    "i18n"
  ],
  "@progress": {
    "friendlyName": "Internationalization"
  },
  "assets": [
    "locales"
  ],
  "sideEffects": [
    "./locales/**/*"
  ],
  "peerDependencies": {
    "@angular/animations": "13 - 16",
    "@angular/common": "13 - 16",
    "@angular/core": "13 - 16",
    "@angular/platform-browser": "13 - 16",
    "@progress/kendo-angular-common": "14.0.0",
    "@progress/kendo-licensing": "^1.0.2",
    "rxjs": "^6.5.3 || ^7.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.1",
    "@progress/kendo-angular-schematics": "14.0.0",
    "@progress/kendo-intl": "^3.1.0"
  },
  "schematics": "./schematics/collection.json",
  "exports": {
    "./locales/*": "./locales/*.js",
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./progress-kendo-angular-intl.d.ts",
      "esm2020": "./esm2020/progress-kendo-angular-intl.mjs",
      "es2020": "./fesm2020/progress-kendo-angular-intl.mjs",
      "es2015": "./fesm2015/progress-kendo-angular-intl.mjs",
      "node": "./fesm2015/progress-kendo-angular-intl.mjs",
      "default": "./fesm2020/progress-kendo-angular-intl.mjs"
    }
  },
  "module": "fesm2015/progress-kendo-angular-intl.mjs",
  "es2020": "fesm2020/progress-kendo-angular-intl.mjs",
  "esm2020": "esm2020/progress-kendo-angular-intl.mjs",
  "fesm2020": "fesm2020/progress-kendo-angular-intl.mjs",
  "fesm2015": "fesm2015/progress-kendo-angular-intl.mjs",
  "typings": "progress-kendo-angular-intl.d.ts"
}